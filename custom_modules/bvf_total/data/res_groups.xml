<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="category_total" model="ir.module.category">
            <field name="name">Total</field>
        </record> 
        <record id="group_agent_administratif" model="res.groups">
            <field name="name">Agents Administratifs</field>
            <field name="category_id" ref="category_total"/>
            <field name="users" eval="[(4,ref('base.user_root'))]"/>
        </record> 
        <record id="group_prescripteur" model="res.groups">
            <field name="name">Prescripteur</field>
             <field name="category_id" ref="category_total"/>
            <field name="users" eval="[(4,ref('base.user_root'))]"/>
        </record>   
        <record id="group_comptables" model="res.groups">
            <field name="name">Comptabilité</field>
             <field name="category_id" ref="category_total"/>
            <field name="users" eval="[(4,ref('base.user_root'))]"/>
        </record>     
        <record id="group_tresoriers" model="res.groups">
            <field name="name">Trésorerie</field>
             <field name="category_id" ref="category_total"/>
            <field name="users" eval="[(4,ref('base.user_root'))]"/>
        </record>
        
        <record id="group_administrateurs" model="res.groups">
            <field name="name">Administrateurs</field>
             <field name="category_id" ref="category_total"/>
             <field name="implied_ids" eval="[(4, [ref('base.user_root'),ref('group_tresoriers'), ref('group_comptables'), ref('group_prescripteur'), ref('group_agent_administratif'), ref('base.group_system')])]"/>
           <!--  <field name="implied_ids" eval="[(4, [ref('base.user_root'),ref('group_tresoriers'), ref('group_comptables'), ref('group_prescripteur'), ref('group_agent_administratif')])]"/> -->
        </record>
    </data>
</odoo>